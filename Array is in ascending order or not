#include<stdio.h>
int main()
{
    int i, arr[i];
    
    printf("Enter 10 Elements : ");
    for (i=0; i<10; i++)
    scanf("%d", &arr[i]);
 
    for(i=0; i<arr[i]-1 ; i++) 
    { if(arr[i]>arr[i+1])
        {
            printf("Array is not sorted in assenting order");
            
        }
    }
     
    printf("Array is sorted in assenting order");
}
