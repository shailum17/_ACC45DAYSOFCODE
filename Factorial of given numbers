#include<stdio.h>
int main ()
{
    int i,n; 
    unsigned long factorial=1;
    printf (" Enter the integer :");
    scanf (" %d ",&n);
    if (n<0)
    {
        printf (" Factorial of negative integer doesn't exist ");
    }
    else 
    {
        for ( i=1;i<=n;++i)
        factorial*=i;
        printf (" factorial of %d=%lu ",n, factorial );
    }
    
    return 0;
}
