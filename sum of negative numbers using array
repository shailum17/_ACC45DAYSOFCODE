#include<stdio.h>
int main()
{
    int i, num, arr[i] , sum=0;
    printf("Enter the size of array : ");
    scanf("%d",&num);
    printf("Enter the elements : ");
    for(i=0;i<num;i++)
    {
        scanf("%d",&arr[i]);
        if (arr[i]<0)
        { 
             sum+=arr[i];
             
        }
        else
        printf("0");
            
       
     }
     printf("%d",sum);
}
