#include<stdio.h>
int main()
{
    int x,i,j,sum=0;
    printf("\n Enter the Integer : ");
    scanf("%d",&x);
    for(i=1;i<=x;i++)
    {
        if (x%i==0)
        {
            for(j=1;j<=i;j++)
            {
                if(i%j==0)
                {
                    sum=sum+j;
                    
                }
            }
        }
        
    }
    printf("\n Sum of all divisor of Integer : %d",sum);
}
